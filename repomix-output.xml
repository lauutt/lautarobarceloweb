<file path="src/theme.css">
:root {
    /* Paleta de colores ne√≥n */
    --neon-pink: #ff00ff;
    --neon-blue: #00ffff;
    --neon-green: #39ff14;
    --neon-yellow: #ffff00;
    --neon-purple: #9d00ff;
    
    /* Colores de fondo */
    --bg-dark: #0a0a0a;
    --bg-darker: #050505;
    --bg-light: #1a1a1a;
    
    /* Tipograf√≠a */
    --pixel-font: 'VT323', 'Press Start 2P', monospace;
    --indie-font: 'Space Mono', 'Courier New', monospace;
    
    /* Efectos */
    --neon-glow: 0 0 5px rgba(255, 255, 255, 0.5), 
                 0 0 10px rgba(255, 255, 255, 0.3), 
                 0 0 15px var(--neon-blue), 
                 0 0 20px var(--neon-blue);
    --neon-pink-glow: 0 0 5px rgba(255, 255, 255, 0.5), 
                      0 0 10px rgba(255, 255, 255, 0.3), 
                      0 0 15px var(--neon-pink), 
                      0 0 20px var(--neon-pink);
    --neon-green-glow: 0 0 5px rgba(255, 255, 255, 0.5), 
                       0 0 10px rgba(255, 255, 255, 0.3), 
                       0 0 15px var(--neon-green), 
                       0 0 20px var(--neon-green);
  }
  
  /* Estilo base para el cuerpo */
  body {
    background-color: var(--bg-dark);
    background-image: 
      linear-gradient(rgba(10, 10, 10, 0.95), rgba(10, 10, 10, 0.95)),
      url('data:image/svg+xml;utf8,<svg width="20" height="20" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><rect width="1" height="1" fill="%23333"/></svg>');
    background-size: 100%, 20px 20px;
    color: #ffffff;
    font-family: var(--indie-font);
  }
  
  /* T√≠tulos con efecto ne√≥n */
  h1, h2, h3 {
    font-family: var(--pixel-font);
    letter-spacing: 1px;
    text-transform: uppercase;
    margin-bottom: 1.5rem;
  }
  
  h1 {
    font-size: 2.5rem;
    color: var(--neon-blue);
    text-shadow: var(--neon-glow);
  }
  
  h2 {
    font-size: 2rem;
    color: var(--neon-pink);
    text-shadow: var(--neon-pink-glow);
  }
  
  h3 {
    font-size: 1.5rem;
    color: var(--neon-green);
    text-shadow: var(--neon-green-glow);
  }
  
  /* Enlaces con efecto ne√≥n */
  a {
    color: var(--neon-blue);
    text-decoration: none;
    transition: all 0.3s ease;
    position: relative;
  }
  
  a:hover {
    color: #ffffff;
    text-shadow: var(--neon-glow);
  }
  
  a:hover::after {
    content: '';
    position: absolute;
    left: 0;
    bottom: -2px;
    width: 100%;
    height: 1px;
    background-color: var(--neon-blue);
    box-shadow: var(--neon-glow);
  }
  
  /* Contenedor principal con efecto de borde pixelado */
  .container {
    margin: 0 auto;
    padding: 20px;
    border: 2px solid rgba(255, 255, 255, 0.1);
    background-color: rgba(0, 0, 0, 0.7);
    box-shadow: 0 0 20px rgba(0, 0, 0, 0.5);
    position: relative;
  }
  
  /* Botones pixelados */
  .pixel-button {
    font-family: var(--pixel-font);
    background-color: transparent;
    border: 2px solid var(--neon-blue);
    color: var(--neon-blue);
    padding: 8px 16px;
    text-transform: uppercase;
    cursor: pointer;
    position: relative;
    overflow: hidden;
    transition: all 0.3s ease;
  }
  
  .pixel-button:hover {
    background-color: var(--neon-blue);
    color: var(--bg-dark);
    box-shadow: var(--neon-glow);
  }
  
  /* Animaci√≥n de escaneo para algunos elementos */
  @keyframes scanline {
    0% {
      transform: translateY(-100%);
    }
    100% {
      transform: translateY(100%);
    }
  }
  
  .scanline {
    position: relative;
    overflow: hidden;
  }
  
  .scanline::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 5px;
    background: linear-gradient(to bottom, 
                               rgba(255, 255, 255, 0.2), 
                               rgba(255, 255, 255, 0));
    animation: scanline 3s linear infinite;
    pointer-events: none;
    z-index: 10;
  }
  
  /* Utilidad para a√±adir brillo ne√≥n a elementos */
  .neon-text-blue { color: var(--neon-blue); text-shadow: var(--neon-glow); }
  .neon-text-pink { color: var(--neon-pink); text-shadow: var(--neon-pink-glow); }
  .neon-text-green { color: var(--neon-green); text-shadow: var(--neon-green-glow); }
  
  .neon-border-blue { 
    border: 1px solid var(--neon-blue); 
    box-shadow: 0 0 5px var(--neon-blue);
  }
  
  .neon-border-pink { 
    border: 1px solid var(--neon-pink); 
    box-shadow: 0 0 5px var(--neon-pink);
  }
  
  .neon-border-green { 
    border: 1px solid var(--neon-green); 
    box-shadow: 0 0 5px var(--neon-green);
  }
</file>

